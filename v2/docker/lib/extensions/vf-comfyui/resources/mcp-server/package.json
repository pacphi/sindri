{
  "name": "comfyui-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for ComfyUI workflow orchestration",
  "type": "commonjs",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "node --test",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "comfyui",
    "stable-diffusion",
    "image-generation",
    "workflow-automation"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "ws": "^8.18.0",
    "chokidar": "^4.0.1",
    "sharp": "^0.33.5",
    "prom-client": "^15.1.3",
    "zod": "^3.23.8",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.12",
    "eslint": "^9.0.0",
    "typescript": "^5.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/claude-flow"
  },
  "author": "Claude Flow Team",
  "license": "MIT",
  "config": {
    "comfyui_url": "http://localhost:8188",
    "output_dir": "/tmp/comfyui-outputs",
    "max_concurrent_workflows": 4,
    "metrics_port": 9091
  }
}
